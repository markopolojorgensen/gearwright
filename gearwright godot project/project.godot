config/features=PackedStringArray("4.4", "GL Compatibility")
config/icon="res://Assets/Icon.png"
config/windows_native_icon="res://Assets/icon.ico"

[autoload]

DataHandler="*res://autoloads_globals_and_singletons/DataHandler.gd"
